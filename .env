<h2>Dashboard</h2>
<p>Welcome to your dashboard, <span id="userEmail"></span>!</p>
 
   // Fetch user email from the server and update the dashboard
    fetch('/user/email')
      .then(response => response.json())
      .then(data => {
        document.getElementById('userEmail').textContent = data.email;
      })
      .catch(error => console.error('Error fetching user email:', error));

